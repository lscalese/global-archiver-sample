{
    "Security.Services" : {
        "%Service_Mirror" : {
            "Enabled" : true
        }
    },
    "SYS.MirrorFailOver" : {
        "Demo" : {
            "Config": {
                "Name" : "Demo",
                "SystemName" : "backup",
                "InstanceName" : "IRIS",
                "AgentAddress" : "${MASTER_APP_NET_IP}",
                "AgentPort" : "2188",
                "AsyncMember" : false,
                "AsyncMemberType" : ""
            },
            "Databases" : [{
                 "Directory" : "/usr/irissys/mgr/myappdata/"
            }],
            "LocalInfo" : {
                "VirtualAddressInterface" : "eth0",
                "MirrorAddress": "${BACKUP_MIRROR_NET_IP}"
            },
            "SSLInfo" : {
                "CAFile" : "/certificates/CA_Server.cer",
                "CertificateFile" : "/certificates/backup_server.cer",
                "PrivateKeyFile" : "/certificates/backup_server.key",
                "PrivateKeyPassword" : "",
                "PrivateKeyType" : "2"
            }
        }
    },
    "Security.SSLConfigs": {
        "%SuperServer": {
            "CAFile": "/certificates/CA_Server.cer",
            "CertificateFile": "/certificates/backup_server.cer",
            "Name": "%SuperServer",
            "PrivateKeyFile": "/certificates/backup_server.key",
            "Type": "1",
            "VerifyPeer": 3
        },
        "%ECPClient": {
            "CAFile": "/certificates/CA_Server.cer",
            "CertificateFile": "/certificates/backup_server.cer",
            "Name": "%ECPClient",
            "PrivateKeyFile": "/certificates/backup_server.key",
            "Type": "0"
        }
    },
    "ECPServers" : {
        "ARCHIVE" : {
            "Name" : "ARCHIVE",
            "Address" : "${ARCHIVE_MIRROR_NET_IP}",
            "Port" : "1972",
            "SSLConfig" : "1"
        }
    },
    "Security.System": {
        "SSLSuperServer":1
    },
    "Databases": {
        "ARCHIVE" : {
            "Directory" : "/usr/irissys/mgr/archive/",
            "Name" : "ARCHIVE",
            "Server" : "ARCHIVE"
        }
    },
    "MapGlobals":{
        "USER": [{
            "Name" : "demo.*",
            "Database" : "MYAPPDATA"
        },{
            "Name" : "lscalese.globalarchive",
            "Database" : "MYAPPDATA"
        },{
            "Name" : "lscalese.globalarcCA13.DataLogD",
            "Database" : "MYAPPDATA"
        },{
            "Name" : "lscalese.globalarcCA13.DataLogI",
            "Database" : "MYAPPDATA"
        },{
            "Name" : "lscalese.globalarcCA13.DataLogC",
            "Database" : "MYAPPDATA"
        }]
    }
}